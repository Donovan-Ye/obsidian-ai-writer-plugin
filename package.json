{
  "name": "obsidian-ai-writer-plugin",
  "version": "0.0.0",
  "description": "A plugin for Obsidian that uses AI to help you write better and faster.",
  "author": "Donovan Ye",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Donovan-Ye/obsidian-ai-writer-plugin"
  },
  "keywords": [],
  "main": "main.js",
  "scripts": {
    "dev": "npm run dev:css && node esbuild.config.mjs",
    "build": "npm run build:css && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "fix": "eslint . --fix",
    "build:css": "tailwindcss -i ./src/main.css -o styles.css",
    "dev:css": "npm run build:css --watch",
    "version": "node version-bump.mjs && npm run fix && git add manifest.json versions.json"
  },
  "dependencies": {
    "@uiw/codemirror-theme-okaidia": "^4.21.25",
    "@uiw/react-codemirror": "^4.21.25",
    "antd": "^5.16.1",
    "i18next": "^23.11.1",
    "lucide-react": "^0.367.0",
    "openai": "^4.33.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.12.2",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "obsidian": "latest",
    "tailwindcss": "^3.4.3",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
